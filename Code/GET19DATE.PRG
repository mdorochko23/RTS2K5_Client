**FUNCTION Get19Date
PARAMETERS  C_CLCODE, N_TAG
LOCAL  OMED AS OBJECT,   lnCurArea AS STRING, d_TXN19 AS DATETIME
lnCurArea=ALIAS()
_SCREEN.MOUSEPOINTER=11
PRIVATE OMED AS OBJECT
OMED=CREATEOBJECT("generic.medgeneric")
SELECT 0


OMED.CLOSEALIAS( "txn19")
C_SQL =" SELECT  [dbo].[GetTxn19DATE]('" +   FIXQUOTE(C_CLCODE) + "','" + STR(N_TAG) + "')"
L_OK =OMED.SQLEXECUTE(C_SQL, "txn19")

IF USED ("txn19")
	d_TXN19=		NVL(TXN19.EXP,'')
ENDIF

_SCREEN.MOUSEPOINTER=0
RELEASE OMED

RETURN TTOD(d_TXN19)
