
FUNCTION CHKFAX 
PARAMETERS cfaxnum
LOCAL L_FAX AS Boolean
L_FAX =.F.


	IF INLIST(ALLTRIM(cfaxnum), '(111)111-1111','(222)222-2222','(333)333-3333','(444)444-4444',;
			'(555)555-5555','(666)666-6666','(777)777-7777','(888)888-8888','(999)999-9999','(000)000-0000')
			
			L_FAX=.F.
	ELSE	
	
			L_FAX=.t.
	ENDIF
	
	
	



RETURN L_FAX
